  /usr/lib/perl5/vendor_perl/5.8/cygwin/auto/Gtk2/Notify/.packlist
  /usr/lib/perl5/vendor_perl/5.8/cygwin/auto/Gtk2/Notify/Notify.bs
  /usr/lib/perl5/vendor_perl/5.8/cygwin/auto/Gtk2/Notify/Notify.dll
  /usr/lib/perl5/vendor_perl/5.8/cygwin/auto/Gtk2/Notify/libNotify.dll.a
